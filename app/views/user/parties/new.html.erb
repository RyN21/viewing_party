<h1> Create New Viewing Party </h1>

<p> Movie Title:</p>
<%= form_for @party do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br />

  <%= f.date_select :dateinstructed, :order => [:day, :month, :year] %>

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.submit %>
<% end %>
